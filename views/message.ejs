<div id="messages">
    <% Object.keys(messages).forEach(function (type) { %>
      <ul class="<%= type %>">
      <% messages[type].forEach(function (message) { %>
        <div class="'alert alert-<%=type%>" style="padding:5px;border-radius:2px"><%= message %></div>
      <% }) %>
      </ul>
    <% }) %>
</div>